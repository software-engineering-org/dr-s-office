<body class="profile-page">
	<nav class="navbar navbar-transparent navbar-fixed-top navbar-color-on-scroll">
    	<div class="container">
        	<!-- Brand and toggle get grouped for better mobile display -->
        	<div class="navbar-header">
        		<button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navigation-example">
            		<span class="sr-only">Toggle navigation</span>
		            <span class="icon-bar"></span>
		            <span class="icon-bar"></span>
		            <span class="icon-bar"></span>
        		</button>
        		<a class="navbar-brand" href="../index.html">Doctor's Clinic</a>
        	</div>

        	<div class="collapse navbar-collapse" id="navigation-example">
        		<ul class="nav navbar-nav navbar-right">
					<li>
						<a href="/signout">
							Logout
						</a>
					</li>
        		</ul>
        	</div>
    	</div>
    </nav>


	<div class="wrapper">
		<div class="header header-filter" style="background-image: url('images/login.png');"></div>

		<div class="main main-raised">
			<div class="profile-content">
				<div class="container">
					<div class="row">
						<div class="profile">
							<div class="avatar">
								<img src="/images/default-avatar.png" alt="Circle Image" class="img-circle img-responsive img-raised">
								<h4 class="title">Name<br>
									<small class="text-muted">Id</small>
								</h4>
							</div>
						</div>
					</div>


					<div class="form-group row">
						<div class="col-md-12">
							<div class="profile-tabs">
								<div class="nav-align-center">

									<div class="tab-content gallery">
										<div class="tab-pane active">
											<div class="row">

												<div class="table-responsive">
													<table class="table">
														<thead>
														<tr>
															<th class="text-center">Item Name</th>
															<th class="text-center">Discription</th>
															<th class="text-center">Unit Cost</th>
														</tr>
														</thead>
														<tbody>
														<tr>
															<td class="text-center"></td>
															<td class="text-center"></td>
															<td class="text-center"></td>
														</tr>

														</tbody>
													</table>
												</div>

												<div class="form-group row">
													<label class="col-sm-2 col-form-label">Total Cost : </label>
													<div class="col-sm-10">
														<p class="form-control-static">data retrieved</p>
													</div>
												</div>



											</div>
										</div>

									</div>
								</div>
							</div>
							<!-- End Profile Tabs -->
						</div>
					</div>
					<div class="form-group row">
						<div class="profile">
							<div class="avatar">
								<img src="/images/default-avatar.png" alt="Circle Image" class="img-circle img-responsive img-raised">
								<h4 class="title">Name<br>
									<small class="text-muted">Id</small>
								</h4>
							</div>
						</div>
					</div>

				</div>
			</div>
		</div>

	</div>



    <footer class="footer">
        <div class="container">
            <nav class="pull-left">
				<ul>
